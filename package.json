{
  "name": "advent-of-code-2022",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "day": "export NODE_OPTIONS=--max_old_space_size=8192 && ts-node --project tsconfig.json src/index.ts",
    "watch-day": "export NODE_OPTIONS=--max_old_space_size=8192 && nodemon --exec \"ts-node --project tsconfig.json src/index.ts\" -e .ts,.js",
    "lint": "eslint . --ext .js,.ts --ignore-path ./.gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andre-brdoch/advent-of-code-2022.git"
  },
  "keywords": [
    "advent of code",
    "aoc",
    "1337",
    "leet"
  ],
  "author": "Andre Brdoch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andre-brdoch/advent-of-code-2022/issues"
  },
  "homepage": "https://github.com/andre-brdoch/advent-of-code-2022#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
